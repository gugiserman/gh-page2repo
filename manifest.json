{
  "name": "Github Page2Repo",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Switch back and forth from a github page to itâ€™s repository.",
  "homepage_url": "https://github.com/gugiserman/gh-page-switch",
  "content_scripts": [{
    "js": [
      "js/script.js"
    ],
    "matches": [
      "http://github.com/*",
      "https://github.com/*",
      "http://*.github.io/*",
      "https://*.github.io/*"
    ]
  }],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Github Page/Repo Switch"
  },
  "permissions": [
    "tabs"
  ]
}
